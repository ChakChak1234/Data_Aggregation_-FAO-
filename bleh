import webbrowser
from selenium import webdriver

#directories
link = 'https://www.'

#download the Chrome driver and save it to a folder - provide the access path below
driver = webdriver.Chrome(executable_path='C:/Users/...')
driver.get(link)


username = driver.find_element_by_id('username')
#xyz = username
username.send_keys('xyz')

password = driver.find_element_by_id('password')
password.send_keys('xyz')

submit_button = driver.find_element_by_name('button')
submit_button.click()